{
    "name": "MACRO_CPSC",
    "run_details": "_0.2-6_12_entmax15",
    "n_gpu": 1,
    "arch": {
        "type": "FinalModel",
        "args": {
            "num_classes": 9,
            "apply_final_activation": false,
            "multi_label_training": true,
            "down_sample": "conv",
            "vary_channels": true,
            "pos_skip": "all",
            "norm_type": "BN",
            "norm_pos": "all",
            "norm_before_act": true,
            "use_pre_activation_design": true,
            "use_pre_conv": true,
            "pre_conv_kernel": 16,
            "dropout_attention": 0.2,
            "heads": 6,
            "gru_units": 12,
            "use_reduced_head_dims": true,
            "attention_activation_function": "entmax15"
        }
    },
    "data_loader": {
        "type": "ECGDataLoader",
        "cross_valid": {
            "enabled": false,
            "k_fold": 10,
            "data_dir": ""
        },
        "args": {
            "data_dir": "data/CinC_CPSC/train/preprocessed/250Hz/60s",
            "batch_size": 64,
            "shuffle": true,
            "validation_split": 0.25,
            "num_workers": 4,
            "pin_memory": false
        },
        "test_dir": "data/CinC_CPSC/test/preprocessed/250Hz/60s"
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "loss": {
        "type": "BCE_with_logits",
        "add_args": {}
    },
    "metrics": {
        "ml": {
            "per_iteration": {
                "sk_subset_accuracy": [
                    "logits"
                ]
            },
            "per_epoch": [
                "weighted_sk_f1",
                "macro_sk_f1"
            ],
            "per_epoch_class_wise": [
                "class_wise_sk_f1"
            ]
        }
    },
    "lr_scheduler": {
        "active": false
    },
    "trainer": {
        "epochs": 300,
        "save_dir": "savedVM/",
        "save_period": 1,
        "verbosity": 2,
        "monitor": "max val_macro_sk_f1",
        "early_stop": 20,
        "tensorboard": true,
        "profiler_active": false
    },
    "SEED": 123
}